#include <unistd.h>
#include <stdio.h>

int ft_putchar(char c)
{
  write(1, &c, 1);
  return (0);
}

void ft_print_comb(void)
{
  int i, j, k, count = 0;

  for (i = 9; i >= 0; i--) {
    for (j = i-1; j >= 0; j--) {
      for (k = j-1; k >= 0; k--) {

	printf("\n%d%d%d", i,j,k);
	printf("\nCount:  %d", count);
	count++;
      }
    }
  }

    /*
    ft_putchar(48);
    */

}


int main()
{
  ft_print_comb();
  return (0);
}
